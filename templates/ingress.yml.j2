---

kind: Ingress
apiVersion: networking.k8s.io/v1beta1
metadata:
  name: pysimple
  namespace: {{ namespace }}
  annotations:
    ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: {{ ingress_host }}
    http:
      paths:
      - path: /
        backend:
          serviceName: pysimple
          servicePort: 8080
